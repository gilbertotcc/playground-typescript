{
  "name": "express-test",
  "version": "1.0.0",
  "description": "Simple Express service",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "ava:test": "tsc --noEmit --pretty -p test/tsconfig.test.json && TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true ava",
    "ava:integrationTest": "tsc --noEmit --pretty -p integrationTest/tsconfig.test.json && TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true ava",
    "test": "npm run ava:test -- -v --serial --verbose --config test/ava.config.cjs test/**/*",
    "integrationTest": "npm run dockerCompose:up && npm run ava:integrationTest -- -v --serial --config integrationTest/ava.config.cjs integrationTest/**/* && npm run dockerCompose:down",
    "dockerCompose:up": "docker-compose up -d",
    "dockerCompose:down": "docker-compose down",
    "check": "npm run test && npm run integrationTest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "ava": "^6.0.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
